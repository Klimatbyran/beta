{
  "name": "beta",
  "type": "module",
  "version": "0.0.101",
  "scripts": {
    "dev": "astro dev",
    "start": "node dist/server/entry.mjs",
    "build": "astro check && astro build",
    "telemetry": "astro telemetry disable",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "prettier --check .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@astrojs/check": "^0.9.3",
    "@astrojs/node": "^8.3.4",
    "@astrojs/react": "^3.6.2",
    "@astrojs/svelte": "^5.7.1",
    "@astrojs/tailwind": "^5.1.1",
    "@astrojs/ts-plugin": "^1.10.2",
    "@fontsource-variable/dm-sans": "^5.1.0",
    "astro": "^4.15.9",
    "bits-ui": "^0.21.15",
    "clsx": "^2.1.1",
    "cmdk-sv": "^0.0.18",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "slugify": "^1.6.6",
    "svelte": "^5.10.0",
    "svelte-hamburgers": "^5.0.0",
    "tailwind-merge": "^2.5.5",
    "tailwind-variants": "^0.3.0",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.7.2",
    "unplugin-icons": "^0.21.0"
  },
  "devDependencies": {
    "@iconify-json/bi": "^1.2.0",
    "@iconify-json/clarity": "^1.2.0",
    "@iconify-json/fa6-solid": "^1.2.0",
    "@iconify-json/icon-park-outline": "^1.2.0",
    "@iconify-json/ion": "^1.2.0",
    "@iconify-json/lucide": "^1.2.5",
    "@iconify-json/maki": "^1.2.0",
    "@iconify-json/material-symbols": "^1.2.1",
    "@iconify-json/mingcute": "^1.2.0",
    "@iconify-json/streamline": "^1.2.0",
    "@iconify-json/zondicons": "^1.2.0",
    "@types/node": "^22.7.0",
    "@types/react": "^18.3.9",
    "@types/react-dom": "^18.3.0",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-svelte": "^3.2.6",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "unplugin-icons": "^0.19.3"
  },
  "overrides": {
    "@melt-ui/svelte": {
      "svelte": "^5.0.0"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "plugins": [
      "prettier-plugin-astro",
      "prettier-plugin-tailwindcss",
      "prettier-plugin-svelte"
    ],
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      },
      {
        "files": "*.svelte",
        "options": {
          "parser": "svelte"
        }
      }
    ]
  }
}
